<div id="<%= dom_id contact %>">
  <p>
    <strong>Name:</strong>
    <%= contact.name %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= contact.title %>
  </p>

  <p>
    <strong>Contact number:</strong>
    <%= contact.phone %>
  </p>

  <p>
    <strong>Client:</strong>
    <%= contact.client_name || contact.contact_type.titleize %>
  </p>

</div>
